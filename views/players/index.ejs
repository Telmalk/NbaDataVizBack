<h1>players</h1>


<% if (result.length == 0) { %>
  <h4>
    0 players
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>id_players</th>
			<th>name</th>
			<th>college</th>
			<th>birthdate</th>
			<th>pictures</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.id_players %>
					</td>
					<td>
						<%= item.name %>
					</td>
					<td>
						<%= item.college %>
					</td>
					<td>
						<%= item.birthdate %>
					</td>
					<td>
						<%= item.pictures %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/players/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/players/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/players/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/players/create">New</a>